{
  "manifest_version": 3,
  "name": "WebSafe Sentinel",
  "version": "1.0",
  "description": "Web security scanning tool",
  "permissions": [
    "activeTab",
    "storage"
  ],


  "web_accessible_resources": [
    {
      "resources": ["index.css", "App.css"],
      "matches": ["<all_urls>"]
    }
  ]
}